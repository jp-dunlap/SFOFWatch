{
  "name": "sfofwatch",
  "version": "1.0.0",
  "description": "SFOFWatch.org is an open-source intelligence project that maps the network of influence driving the State Financial Officers Foundation (SFOF) and its associated political agenda.",
  "main": ".eleventy.js",
  "scripts": {
    "start": "npx @11ty/eleventy --serve --quiet",
    "build": "npm-run-all build:data eleventy",
    "build:data": "node netlify/functions/update-legislation.js",
    "eleventy": "npx @11ty/eleventy",
    "db:migrate": "node-pg-migrate"
  },
  "keywords": [
    "SFOF",
    "State Financial Officers Foundation",
    "ESG",
    "Anti-ESG",
    "State Treasurers"
  ],
  "author": "JP Dunlap",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@octokit/rest": "^20.0.2",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "html-minifier-terser": "^7.2.0",
    "markdown-it": "^14.1.0",
    "netlify-cli": "^17.22.1",
    "node-pg-migrate": "^7.5.2",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
